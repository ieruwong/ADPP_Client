page{
  background-color: #dedede;
  padding: 20rpx;
}
.search_row{
  height: 70rpx;
  display: flex;
  padding: 0;
  margin: 0 25rpx 0 0;
}
.search_row input{
  flex: 1;
  height: 100%;
  background-color: #fff;
  border-radius: 10rpx;
  padding-left: 30rpx;
}
.search_row .btn{
  width: 120rpx;
  height: 100%;
  font-size: 25rpx;
  padding: 0;
  margin: 0 10rpx;
  justify-content: center;
  display: flex;
  align-items: center;
}