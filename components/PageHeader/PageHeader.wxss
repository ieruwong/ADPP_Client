/******************************顶栏******************************/
.header{
  width:750rpx;
  height: 90rpx;
}
.hdCon{
  display: flex;/*//将对象作为弹性伸缩盒显示*/
  justify-content: space-between;/*图标两端对齐*/
  /*元素居中*/
  align-items: center;
  height: 100%;
  padding: 0 26rpx;
}
.logo,.hdCon .search{
  height: 70rpx;
}
.images{
  height: 100%;
  border-radius:50rpx;
}
.search{
  position: relative;
}
.search .btn{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  opacity: 0;
}